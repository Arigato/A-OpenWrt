# 设置固件大小:
CONFIG_TARGET_ramips=y
CONFIG_TARGET_ramips_mt7621=y
CONFIG_TARGET_ramips_mt7621_DEVICE_phicomm_k2p=y

CONFIG_PACKAGE_luci-app-turboacc=y
CONFIG_PACKAGE_luci-app-turboacc_INCLUDE_flow-offload=y
CONFIG_PACKAGE_luci-app-turboacc_INCLUDE_shortcut-fe=n

CONFIG_KERNEL_MIPS_FPU_EMULATOR=y

CONFIG_PACKAGE_luci-app-ssr-plus_INCLUDE_Shadowsocks=y
CONFIG_PACKAGE_luci-app-ssr-plus_INCLUDE_redsocks2=y #redsocks2代理
CONFIG_PACKAGE_luci-app-ddns=y #动态域名 DNS（集成阿里DDNS客户端）


# 其他需要安装的软件包:


# wireguard
CONFIG_PACKAGE_kmod-wireguard=y #VPN
